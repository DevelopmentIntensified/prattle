{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "no-console": 1, // Means warning
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ], // Means error
    "no-use-before-define": "off",
    "@typescript-eslint/no-use-before-define": "warn"
  }
}
